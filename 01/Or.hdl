 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	// formula: ~(~(a)~(b))
	Not(in=a, out=a0);
	Not(in=b, out=b0);
	Nand(a=a0, b=b0, out=out);
}
